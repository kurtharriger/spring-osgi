Bundle-Name: Spring-OSGi-Test-Framework
Bundle-Version: 1.0
Bundle-SymbolicName: org.springframework.osgi.test
Bundle-Activator: org.springframework.osgi.test.OsgiJUnitService
Import-Package: junit.framework,
  org.springframework.osgi.io,
  edu.emory.mathcs.backport.java.util.concurrent
DynamicImport-Package: javax.swing, javax.swing.border,
  javax.swing.event, javax.swing.text, javax.swing.tree,
  org.eclipse.core.runtime.adaptor,
  org.knopflerfish.framework,
  org.apache.felix.framework,
  org.apache.felix.framework.util,
  org.apache.felix,main, *
Export-Package: org.springframework.osgi.test, 
  org.springframework.osgi.test.runner
Bundle-ClassPath: target/classes/,.

