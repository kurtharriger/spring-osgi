digraph {
    nodesep=.60;
    o [shape=point];
    i [label=" INSTALLED "];
    r [label="RESOLVED"];
    ui [label=" UNINSTALLED "];
    sa [label=" STARTING "];
    a [label="ACTIVE"];
    so [label=" STOPPING "];

        
    subgraph cluster_0 {
        color="white"
        o -> i [label=" install "];
        i -> i [label=" update"];
        i -> r [label="resolve"];
        r -> i [label="update"];
        r -> ui [label="uninstall "];
        i -> ui [label="uninstall "];
    }
    
    subgraph cluster_1 {
        color="white"
        r -> sa [label="start"];
        sa -> a ;
        sa -> r [label="exception"];
        a -> so [label=" stop"];
        so -> r;
    }
    

}