<?xml version="1.0"?>
<project>
  <parent>
    <artifactId>spring-framework-required-libraries</artifactId>
    <groupId>org.springframework.osgi</groupId>
    <version>1.0-m3</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>

  <groupId>org.springframework.osgi</groupId>
  <artifactId>commons-collections.osgi</artifactId>
  <packaging>bundle</packaging>
  <name>Jakarta Commons Collections (OSGi version)</name>
  <version>3.2-SNAPSHOT</version>
  <url>http://www.springframework.org/osgi</url>

  <properties>
    <unpack.version>3.2</unpack.version>
    <spring.osgi.export.pkg>org.apache.commons.collections*</spring.osgi.export.pkg>
    <!-- !org.apache.commons.collections,-->
    <spring.osgi.import.pkg>*</spring.osgi.import.pkg>
  </properties> 
  
  <dependencies>
    <dependency>
      <groupId>commons-collections</groupId>
      <artifactId>commons-collections</artifactId>
      <version>3.2</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
</project>