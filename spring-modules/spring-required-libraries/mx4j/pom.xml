<?xml version="1.0"?>
<project>
  <parent>
    <artifactId>spring-framework-required-libraries</artifactId>
    <groupId>org.springframework.osgi</groupId>
    <version>1.0-m3</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>

  <groupId>org.springframework.osgi</groupId>
  <artifactId>mx4j</artifactId>
  <packaging>bundle</packaging>
  <name>mx4j(OSGi version)</name>
  <version>3.0.2-SNAPSHOT</version>
  <url>http://www.springframework.org/osgi</url>
  
  <properties>
    <unpack.version>3.0.2</unpack.version>
    <spring.osgi.export.pkg>mx4j*, javax.management*</spring.osgi.export.pkg>
    <spring.osgi.import.pkg>
    org.w3c.dom*;resolution:=optional,
    javax.xml.*;resolution:=optional,
    org.apache.bcel*;resolution:=optional,
    org.apache.log4j*;resolution:=optional,
    *
    </spring.osgi.import.pkg>
  </properties>

 <dependencies>
   <dependency>
    <groupId>mx4j</groupId>
    <artifactId>mx4j</artifactId>
    <version>3.0.2</version>
   </dependency> 
 </dependencies>
</project>