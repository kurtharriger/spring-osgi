<?xml version="1.0"?>
<project>
  <parent>
    <artifactId>spring-framework-required-libraries</artifactId>
    <groupId>org.springframework.osgi</groupId>
    <version>1.0-m2</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>

  <groupId>org.springframework.osgi</groupId>
  <artifactId>hsqldb.osgi</artifactId>
  	<packaging>bundle</packaging>
  <name>HSQLDB (OSGi version)</name>
  <version>1.8.0.7</version>
  <url>http:// www.springframework.org</url>

  <properties>
    <unpack.version>1.8.0.7</unpack.version>
    <spring.osgi.export.pkg>org.hsqldb*</spring.osgi.export.pkg>
    <spring.osgi.import.pkg>
    javax.*;resolution:=optional,
    com.sun*;resolution:=optional,
    *
    </spring.osgi.import.pkg>
  </properties> 
    
  <dependencies>
    <dependency>
      <groupId>hsqldb</groupId>
      <artifactId>hsqldb</artifactId>
      <version>1.8.0.7</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
</project>