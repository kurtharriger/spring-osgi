<?xml version="1.0"?><project>
  <parent>
    <artifactId>spring-framework-required-libraries</artifactId>
    <groupId>org.springframework.osgi</groupId>
    <version>1.0-m2</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.springframework.osgi</groupId>
  <artifactId>struts.osgi</artifactId>
  <packaging>bundle</packaging>
  <name>Struts (OSGi version)</name>
  <version>1.2.9-SNAPSHOT</version>
  <url>http://www.springframework.org/osgi</url>
  
  <properties>
    <unpack.version>1.2.9</unpack.version>
    <spring.osgi.export.pkg>org.apache.struts*</spring.osgi.export.pkg>
    <spring.osgi.import.pkg>
    org.xml.sax;resolution:=optional,
    antlr*;resolution:=optional,
    *
    </spring.osgi.import.pkg>
    
  </properties>
  
  <dependencies>
    <dependency>
      <groupId>struts</groupId>
      <artifactId>struts</artifactId>
      <version>1.2.9</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
</project>