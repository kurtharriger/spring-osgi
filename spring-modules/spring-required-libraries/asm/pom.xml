<?xml version="1.0"?>
<project>
  <parent>
    <artifactId>spring-framework-required-libraries</artifactId>
    <groupId>org.springframework.osgi</groupId>
    <version>1.0-m2-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>

  <groupId>org.springframework.osgi</groupId>
  <artifactId>asm.osgi</artifactId>
  <packaging>bundle</packaging>
  <name>asm (OSGi version)</name>
  <version>2.2.3-SNAPSHOT</version>
  <url>http://www.springframework.org/osgi</url>
  
  <properties>
    <unpack.version>2.2.3</unpack.version>
    <spring.osgi.export.pkg>org.objectweb.asm*</spring.osgi.export.pkg>
    <spring.osgi.import.pkg>
    org.xml.sax*;resolution:=optional,
    javax.xml.*;resolution:=optional,
    *
    </spring.osgi.import.pkg>
  </properties>

  <dependencies>
    <dependency>
      <groupId>asm</groupId>
      <artifactId>asm-all</artifactId>
      <version>2.2.3</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
</project>