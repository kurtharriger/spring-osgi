<?xml version="1.0"?>
<project>
	<parent>
		<artifactId>spring-modules</artifactId>
		<groupId>org.springframework.osgi</groupId>
		<version>2.1-m2-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.springframework.osgi</groupId>
	<artifactId>spring-web</artifactId>
	<packaging>bundle</packaging>
	<name>Spring web module (OSGi version)</name>
	<version>${project.parent.version}</version>
	<url>http://www.springframework.org</url>

    <properties>
       <!-- optional imports-->
	   <spring.osgi.import.pkg>
   com.oreilly.servlet*;resolution:=optional,
   javax.faces*;resolution:=optional,
   javax.servlet.jsp.el*;resolution:=optional,
   org.apache.commons.fileupload*;resolution:=optional,
   org.apache.log4j*;resolution:=optional,
   org.apache.taglibs.standard.lang.support*;resolution:=optional,
   org.xml.sax;resolution:=optional,
   *
      </spring.osgi.import.pkg>
    </properties>
</project>