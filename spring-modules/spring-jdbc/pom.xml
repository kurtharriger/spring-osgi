<?xml version="1.0"?>
<project>
	<parent>
		<artifactId>spring-modules</artifactId>
		<groupId>org.springframework.osgi</groupId>
		<version>2.0.5-osgi-m2</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.springframework.osgi</groupId>
	<artifactId>spring-jdbc</artifactId>
	<packaging>bundle</packaging>
	<name>Spring JDBC module (OSGi version)</name>
	<version>${project.parent.version}</version>
	<url>http://www.springframework.org</url>

  <properties>
	   <!-- optional imports-->
	   <spring.osgi.import.pkg>
	   com.mchange.v2.c3p0;resolution:=optional,
	   com.sun.rowset;resolution:=optional,
       org.apache.commons.dbcp;resolution:=optional,
       org.enhydra.jdbc.core;resolution:=optional,
       weblogic.jdbc.extensions;resolution:=optional,
       javax.naming;resolution:=optional,
       javax.transaction;resolution:=optional
	   </spring.osgi.import.pkg>
  </properties>

</project>