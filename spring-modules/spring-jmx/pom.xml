<?xml version="1.0"?>
<project>
	<parent>
		<artifactId>spring-modules</artifactId>
		<groupId>org.springframework.osgi</groupId>
		<version>2.1-m2-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.springframework.osgi</groupId>
	<artifactId>spring-jmx</artifactId>
	<packaging>bundle</packaging>
	<name>Spring JMX module (OSGi version)</name>
	<version>${project.parent.version}</version>
	<url>http://www.springframework.org</url>


   <properties>
   	   <!-- optional imports-->
	   <spring.osgi.import.pkg>
		weblogic.management;resolution:=optional,
		org.aopalliance;resolution:=optional,
		javax.management.remote*;resolution:=optional,
		javax.naming;resolution:=optional,
		org.springframework.jndi*;resolution:=optional,
		*
       </spring.osgi.import.pkg>
   </properties>
 
</project>