<?xml version="1.0"?>
<project>
	<parent>
		<artifactId>spring-modules</artifactId>
		<groupId>org.springframework.osgi</groupId>
		<version>2.1-m2-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.springframework.osgi</groupId>
	<artifactId>spring-mock</artifactId>
	<packaging>bundle</packaging>
	<name>Spring mock module (OSGi version)</name>
	<version>${project.parent.version}</version>
	<url>http://www.springframework.org</url>

    <properties>
	   <!-- optional imports-->
	   <spring.osgi.import.pkg>
  javax.naming*;resolution:=optional,
  javax.servlet*;resolution:=optional,
  javax.portlet;resolution:=optional,
  javax.sql*;resolution:=optional,
  javax.persistence*;resolution:=optional,
  org.apache.taglibs.standard.lang.support*;resolution:=optional,
  org.aspectj.weaver.loadtime*;resolution:=optional,
  org.springframework.jndi*;resolution:=optional;version="${spring.import.version}",  
  org.springframework.dao*;resolution:=optional;version="${spring.import.version}",
  org.springframework.jdbc.core*;resolution:=optional;version="${spring.import.version}",
  org.springframework.transaction*;resolution:=optional;version="${spring.import.version}",
  org.springframework.orm.jpa*;resolution:=optional;version="${spring.import.version}",
  org.springframework.instrument.classloading*;resolution:=optional;version="${spring.import.version}",
  org.springframework.web*;resolution:=optional;version="${spring.import.version}",
  *
		</spring.osgi.import.pkg>
    </properties>

</project>